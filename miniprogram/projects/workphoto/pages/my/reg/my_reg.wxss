page {
  height: 100%;
  width: 100%;
  margin: 0;
  padding: 0;
  overflow: hidden;
}
.reg {
  height: 100%;
  width: 750rpx;
  background-size: 100% 100%;
  background-image: url('https://6163-achang-8gptgjxj307299e8-1322978268.tcb.qcloud.la/%E8%83%8C%E6%99%AF.png?sign=1d8b7696bbc0d27e09dc0510a1fd38b4&t=1707615112');
}
.reg .logo {
  width: 160rpx;
  height: 160rpx;
  margin-top: 280rpx;
  margin-bottom: 150rpx;
  margin-left: 295rpx;
}

.reg .phone {
  margin: 0rpx auto;
  width: 640rpx;
  height: 100rpx;
  border-radius: 100rpx;
  font-weight: 500;
  background-color: #585CE5;
  line-height: 100rpx;
}

.reg .green_goon {
  width: 640rpx;
  height: 100rpx;
  border-radius: 100rpx;
  line-height: 100rpx;
  font-weight: 500;
  background-color: #585CE5;
}

.reg .down {
  position: fixed;
  bottom: 100rpx;
  margin-left: 90rpx;
}

.form-ac .form-ac-item {
  margin: 0 auto;
  /* margin-bottom: 60rpx; */
  width: 640rpx;
  height: 100rpx;
  border-radius: 100rpx;
  font-weight: 500;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2px 20px rgba(0, 148, 156, 0.15);
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}

.form-ac .form-ac-item input {
  width: 70%;
}
.form-ac .form-ac-item .seconds {
  flex: 1;
  color: rgba(75, 81, 242, 1);
  text-align: center;
  margin-left: 10rpx;
}

.tip-text {
  color: red;
  width: 580rpx;
  margin: 10rpx auto;
  margin-bottom: 20rpx;
  height: 30rpx;
}