@import "../../../style/base/comm.wxss"; 
@import "../../../style/public/project.wxss";  

.backgroundimage {
  width: 140rpx;
  height: 100%;
  background-color: #e1e3e6;
  display: inline-block;
  border-radius: 5rpx;
  position: relative;
}

.backgroundimage image {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 30rpx;
  height: 30rpx;
}

.avatar {
  width: 56rpx;
  height: 56rpx;
  background-color: #e1e3e6;
  display: inline-block;
  border-radius: 50%;
  overflow: hidden;
  position: relative;
}

.avatar image {
  width: 100%;
  height: 100%;
}