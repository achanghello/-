.editor-node-wrapper .editor-pictures {
  width: 720rpx;
  min-height: 380rpx;
  padding: 0rpx;

  background-color: #fff;
  border-radius: 16rpx;
  box-shadow: 0rpx 4rpx 24rpx  rgba(0, 109, 204, 0.06);
  margin: 10rpx auto;
  /* padding: 20rpx; */
  /* display: flex; */
}

.editor-node-wrapper .editor-pictures .item {
  width: 130rpx;
  height: 175rpx;
  border-radius: 14rpx;
  overflow: hidden;
  display: inline-block;
  margin-left: 12rpx;
  margin-top: 5rpx;
  background-color: #000;

  position: relative;
}
.editor-node-wrapper .editor-pictures .item.pic .delete {
  position: absolute;
  background-color: rgba(100, 100, 100, 0.8);
  width: 30rpx;
  height: 30rpx;
  right: 0;
  top: 0;
  border-radius: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.editor-node-wrapper .editor-pictures .item.pic image{
  width: 100%;
  height: 100%;
}

.editor-node-wrapper .editor-pictures .add {
  background: rgba(237, 239, 242, 1);
  display: inline-block;
  position: relative;
}

.editor-pictures .add image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}